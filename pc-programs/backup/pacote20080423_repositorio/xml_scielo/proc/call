C:\SciELO\xml_scielo\proc>echo put PubMed\bjmbrv36n9.xml  1>>seq.txt 

C:\SciELO\xml_scielo\proc>echo cd holdings  1>>seq.txt 

C:\SciELO\xml_scielo\proc>echo put PubMed\journals_bjmbrv36n9.xml  1>>seq.txt 

C:\SciELO\xml_scielo\proc>echo quit  1>>seq.txt 

C:\SciELO\xml_scielo\proc>ftp -i -n ftp-private.ncbi.nih.gov  0<seq.txt 

