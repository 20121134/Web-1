<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="period">		<xsl:if test="position()!=1">;</xsl:if>		<xsl:if test="not(end)">			<xsl:apply-templates select="." mode="display-msg-since"/>		</xsl:if>		<xsl:apply-templates select=".//begin"/>		<xsl:choose>			<xsl:when test=".//end"> - <xsl:apply-templates select="end"/> (<xsl:apply-templates select="end" mode="motive"/>)			</xsl:when>			<xsl:otherwise>				<!--xsl:apply-templates select="." mode="display-msg-current"/-->			</xsl:otherwise>		</xsl:choose>	</xsl:template>	<xsl:template match="end | begin">		<xsl:value-of select="substring(@date,1,4)"/>	</xsl:template>	<xsl:template match="end[@status='D' or @status='T' ]" mode="motive">		<xsl:choose>			<xsl:when test="$interfaceLang='pt' ">Terminado </xsl:when>			<xsl:when test="$interfaceLang='es' ">Terminado </xsl:when>			<xsl:when test="$interfaceLang='en' ">Deceased </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="end[@status='S']" mode="motive">		<xsl:choose>			<xsl:when test="$interfaceLang='pt'">Suspenso </xsl:when>			<xsl:when test="$interfaceLang='es'">Sospendido </xsl:when>			<xsl:when test="$interfaceLang='en'">Suspended </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*" mode="display-msg-certified">		<xsl:choose>			<xsl:when test="$interfaceLang='pt'">Certificada </xsl:when>			<xsl:when test="$interfaceLang='es'">Certificada </xsl:when>			<xsl:when test="$interfaceLang='en'">Certified </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*" mode="display-msg-since">		<xsl:choose>			<xsl:when test="$interfaceLang='pt'">desde </xsl:when>			<xsl:when test="$interfaceLang='es'">desde </xsl:when>			<xsl:when test="$interfaceLang='en'">since </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*" mode="display-msg-current">		<xsl:choose>			<xsl:when test="$interfaceLang='pt'">atual</xsl:when>			<xsl:when test="$interfaceLang='es'">actual</xsl:when>			<xsl:when test="$interfaceLang='en'">current</xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*" mode="display-msg-current-list">		<xsl:param name="count"/>		<p>			<xsl:choose>				<xsl:when test="$interfaceLang='pt'">Títulos correntes</xsl:when>				<xsl:when test="$interfaceLang='es'">Títulos vigentes</xsl:when>				<xsl:when test="$interfaceLang='en'">Current titles</xsl:when>			</xsl:choose> - <xsl:value-of select="$count"/>&#160;			<xsl:choose>				<xsl:when test="$interfaceLang='pt'"> periódicos listados</xsl:when>				<xsl:when test="$interfaceLang='es'">seriadas listadas</xsl:when>				<xsl:when test="$interfaceLang='en'">serials listed</xsl:when>			</xsl:choose>		</p>	</xsl:template>	<xsl:template match="*" mode="display-msg-not-current-list">		<xsl:param name="count"/>		<p>			<xsl:choose>				<xsl:when test="$interfaceLang='pt'">Títulos não correntes</xsl:when>				<xsl:when test="$interfaceLang='es'">Títulos no vigentes</xsl:when>				<xsl:when test="$interfaceLang='en'">Not current titles</xsl:when>			</xsl:choose> - <xsl:value-of select="$count"/>&#160;			<xsl:choose>				<xsl:when test="$interfaceLang='pt'"> periódicos listados</xsl:when>				<xsl:when test="$interfaceLang='es'">seriadas listadas</xsl:when>				<xsl:when test="$interfaceLang='en'">serials listed</xsl:when>			</xsl:choose>		</p>	</xsl:template>	<xsl:template match="*|@*" mode="display-msg-continue-as">		<xsl:choose>			<xsl:when test="$interfaceLang='en'">Continued as </xsl:when>			<xsl:when test="$interfaceLang='pt'">Continua como </xsl:when>			<xsl:when test="$interfaceLang='es'">Continua como </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*|@*" mode="display-msg-deceased">		<xsl:choose>			<xsl:when test="$interfaceLang='en'">Publication finished </xsl:when>			<xsl:when test="$interfaceLang='pt'">Publicação finalizada </xsl:when>			<xsl:when test="$interfaceLang='es'">Publicación finalizada </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="*|@*" mode="display-msg-interrupted">		<xsl:choose>			<xsl:when test="$interfaceLang='en'">Indexing interrupted by committee </xsl:when>			<xsl:when test="$interfaceLang='pt'">Indexação interrompida pelo Comitê </xsl:when>			<xsl:when test="$interfaceLang='es'">Indización interrumpida por el comité </xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="current-status/@status[.='C']">	</xsl:template>	<xsl:template match="current-status/@status[.='S']">		<xsl:apply-templates select="." mode="display-msg-interrupted"/>	</xsl:template>	<xsl:template match="current-status/@status[.='D' or .='T']">		<xsl:apply-templates select="." mode="display-msg-deceased"/>	</xsl:template>	<xsl:template match="SERIAL" mode="display-status-info">		<!--xsl:apply-templates select="." mode="display-status-history"/-->	</xsl:template>	<xsl:template match="SERIAL[.//current-status/@status!='C']" mode="display-status-info">			&#160;<!--&#160; [ -->- <xsl:choose>			<xsl:when test=".//NEWTITLE">				<span class="status-info-alphabetic-list">					<xsl:apply-templates select="." mode="display-msg-continue-as"/>					<xsl:apply-templates select=".//NEWTITLE"/>				</span>			</xsl:when>			<xsl:when test=".//current-status/@status='D' or .//current-status/@status='T' ">				<span class="status-info-alphabetic-list">					<xsl:apply-templates select="." mode="display-msg-deceased"/>					<xsl:choose>						<xsl:when test="$interfaceLang='en'">in </xsl:when>						<xsl:when test="$interfaceLang='pt'">em </xsl:when>						<xsl:when test="$interfaceLang='es'">en </xsl:when>					</xsl:choose>					<xsl:call-template name="ShowDate">						<xsl:with-param name="DATEISO" select=".//@date"/>						<xsl:with-param name="LANG" select="//SERIALLIST/CONTROLINFO/LANGUAGE"/>					</xsl:call-template>				</span>			</xsl:when>			<xsl:when test=".//current-status/@status='S'">				<span class="status-info-alphabetic-list">					<xsl:apply-templates select="." mode="display-msg-interrupted"/>					<xsl:choose>						<xsl:when test="$interfaceLang='en'">in </xsl:when>						<xsl:when test="$interfaceLang='pt'">em </xsl:when>						<xsl:when test="$interfaceLang='es'">en </xsl:when>					</xsl:choose>					<xsl:call-template name="ShowDate">						<xsl:with-param name="DATEISO" select=".//@date"/>						<xsl:with-param name="LANG" select="//SERIALLIST/CONTROLINFO/LANGUAGE"/>					</xsl:call-template>				</span>			</xsl:when>			<xsl:otherwise>			</xsl:otherwise>		</xsl:choose>		<!-- ]			<br/>		-->	</xsl:template>	<xsl:template match="*[.//journal-status-history]" mode="display-status-history">		<img src="xsl/info.JPG">			<xsl:attribute name="alt"><xsl:apply-templates select="." mode="display-msg-certified"/><xsl:apply-templates select=".//period"/></xsl:attribute>		</img>	</xsl:template>	<xsl:template match="*" mode="journal-info">		<xsl:if test=".//current-status/@status!='C'">			<table>				<tbody>					<tr>						<td>							<xsl:apply-templates select=".//current-status/@status"/>						</td>					</tr>					<xsl:if test="//journal-status-history//CHANGESINFO">						<TR>							<TD>								<BR/>								<xsl:apply-templates select="//journal-status-history//CHANGESINFO">									<xsl:with-param name="LANG" select="//CONTROLINFO/LANGUAGE"/>								</xsl:apply-templates>							</TD>						</TR>					</xsl:if>				</tbody>			</table>		</xsl:if>	</xsl:template></xsl:stylesheet>